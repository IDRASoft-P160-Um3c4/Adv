CREATE TABLE "ESERVICOM"."MYREMPRESA"
(
IEJERCICIOINS smallint NOT NULL,
ICVEOFICINA smallint NOT NULL,
IFOLIORPMN int NOT NULL,
IPARTIDA int NOT NULL,
CRFC varchar(13),
ICVEPERSONA bigint,
CCALLE varchar(100),
CNUMEXTERIOR varchar(20),
CNOMRAZONSOCIAL varchar(150),
CNUMINTERIOR varchar(20),
CCOLONIA varchar(75),
CCODPOSTAL varchar(5),
ICVEENTIDADFED smallint,
ICVEMUNICIPIO smallint,
ICVELOCALIDAD smallint,
ICVEPAIS smallint,
DPARTEXTRANJERA decimal(4,2),
CONSTRAINT SQL070207012505180 PRIMARY KEY (IEJERCICIOINS,ICVEOFICINA,IFOLIORPMN,IPARTIDA)
)
;

CREATE TABLE "ESERVICOM"."MYRCAPITANIA"
(
   ICVEOFICINA smallint NOT NULL,
   ICVECAPITANIAPARAMAT smallint NOT NULL,
   CDSCCAPITANIA varchar(75),
   LOFICINAREGISTRAL smallint DEFAULT 0 NOT NULL,
   CNOMREGISTRADOR varchar(75),
   LACTIVO smallint DEFAULT 0 NOT NULL,
   LESREGIONAL smallint DEFAULT 0 NOT NULL,
   ICVEOFICINAREGIONAL smallint,
   CSIGLASOFICINA varchar(2),
   LOFICINAINSPECCION smallint DEFAULT 0 NOT NULL,
   CHORASDIF varchar(8),
   LDERROTERO smallint DEFAULT 0 NOT NULL,
   CONSTRAINT SQL070207012501010 PRIMARY KEY (ICVEOFICINA,ICVECAPITANIAPARAMAT)
);

